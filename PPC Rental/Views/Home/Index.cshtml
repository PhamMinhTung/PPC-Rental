@model IEnumerable<PPC_Rental.Models.PROPERTY>
@{
    ViewBag.Title = "Home Page";
}

<!--Header-->

<div class="container-fluid top-header">
    <div class="row">
       
        <div class="inner-bg text-center">
            <p></p>
        </div>
    </div>
    <!--Khung tìm kiếm-->
    <head>
        <title>Table</title>
        <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">
        <link rel="stylesheet" type="text/css" href="~/Content/myStyle.css">
    </head>
    <body>
        <div class="container-fluid">
            <link rel="stylesheet" type="text/css" href="~/Content/bootstrap.min.css">
            <link rel="stylesheet" type="text/css" href="~/Content/myStyle.css">
            <div class="row">
                <div class="col-md-12 me">
                    <div class="col-md-12 header">
                        <p>Find you place</p>
                    </div>
                    <div class="col-md-12 content">
                        <div class="col-md-3 sub_ct">
                            <div class="col-md-12">
                                <label class="" for="orderBy"></label>
                                <select class="form-control">
                                    <option placeholder="Quận">Quận</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                     
                                </select>
                            </div>
                            <hr>
                            <div class="col-md-12">
                                <label class="" for="orderBy"></label>
                                <select class="form-control">
                                    <option placeholder="Đường">Đường</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3 sub_ct">
                            <div class="col-md-12">
                                <label class="" for="orderBy"></label>
                                <select class="form-control">
                                    <option placeholder="Phòng tắm">Phòng tắm</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <hr>
                            <div class="col-md-12">
                                <label class="" for="orderBy"></label>
                                <select class="form-control">
                                    <option placeholder="Bãi đậu xe">Bãi đậu xe</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3 sub_ct">
                            <div class="col-md-12">
                                <label class="" for="orderBy"></label>
                                <select class="form-control">
                                    <option placeholder="Loại dự án">Loại dự án</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                            <hr>
                            <div class="col-md-12">
                                <label class="" for="orderBy"></label>
                                <select class="form-control">
                                    <option placeholder="Giá">Giá</option>
                                    <option>1</option>
                                    <option>2</option>
                                    <option>3</option>
                                    <option>4</option>
                                    <option>5</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3 sub_ct">
                            <div class="col-md-12">

                                <input type="search" class="form-control" id="Search" placeholder="Search" style="margin-top: 20px">
                            </div>
                            <hr>
                            <div class="col-md-12"><input class="btnsearch" type="button" value="Search" style="margin-top: 20px; height:30px"></div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </body>
    <!--List Project of Company-->
    <hr />
    <h2 style="margin-left: 450px;color:red">"Products of Company"</h2>
    <div class="row">
        @foreach (var Item in Model)
        { 
        <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
                <img src="~/Images/Item/@Item.Avatar" alt="...">
                <div class="caption">
                    <h3>@Item.PropertyName</h3>
                    <p><a href="~/ViewDetailProject/ViewDetailProject/@Item.ID" class="btn btn-primary" role="button">Xem chi tiết</a> <a href="#" class="btn btn-default" role="button">Liên hệ</a></p>
                </div>
            </div>
        </div>
        }
    </div>